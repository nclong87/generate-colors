  },
  "scripts": {
    "lint": "npm run lint:lib && npm run lint:cli",
    "lint:cli": "eslint cli",
    "lint:lib": "eslint lib",
    "docs": "NODE_ENV=produciton webpack --config webpack.config.js",
    "test": "mocha --compilers js:babel-register",
    "coverage": "nyc --reporter=lcov --reporter=text npm test",
    "build": "npm run build:lib && npm run build:cli",
    "build:cli": "babel --presets latest -d bin/ cli/",
    "build:lib": "babel --presets latest -d dist/ lib/",
    "build:runtime": "babel --presets latest --plugins transform-runtime -d dist/ lib/",
    "prepare": "npm run lint && npm test && npm run build",
    "release": "np"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-latest-minimal": "^1.1.2",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "chai": "^4.2.0",
    "core-js": "^2.4.1",
    "eslint": "^6.6.0",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^4.0.0-alpha",
    "mocha": "^5.2.0",
    "np": "^3.0.4",
    "nyc": "^14.1.1",
    "prompt": "^1.0.0",
    "terser-webpack-plugin": "^1.1.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "homepage": "https://github.com/sudkumar/generate-colors#readme",
